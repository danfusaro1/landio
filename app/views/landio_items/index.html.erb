<p id="notice"><%= notice %></p>

<h1>Listing Landio Items</h1>

<table>
  <thead>
    <tr>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @landio_items.each do |landio_item| %>
      <tr>
        <td><%= landio_item.description %></td>
        <td><%= link_to 'Show', landio_item %></td>
        <td><%= link_to 'Edit', edit_landio_item_path(landio_item) %></td>
        <td><%= link_to 'Destroy', landio_item, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Landio item', new_landio_item_path %>
